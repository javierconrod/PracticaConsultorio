<UserControl x:Class="PracticaConsultorio.ElementoConsultaControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PracticaConsultorio"
             mc:Ignorable="d" 
             d:DesignHeight="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="lblSintomasHistorial" Grid.Column="0" Grid.Row="0" Text="Síntomas"></TextBlock>
        <TextBlock x:Name="lblRecetaHistorial" Grid.Column="0" Grid.Row="1" Text="Receta"></TextBlock>
        <TextBlock x:Name="lblDiagnosticoHistorial" Grid.Column="1" Grid.Row="0" Text="Diagnóstico"></TextBlock>
        <TextBlock x:Name="lblFechaHistorial" Grid.Column="1" Grid.Row="1" Text="Fecha"></TextBlock>
        
        
    </Grid>
</UserControl>
